<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp2.0</TargetFramework>
    <Authors>velddev</Authors>
    <Company>velddev</Company>
    <Product>Miki.Data.Prometheus</Product>
    <Description>simplified Statsd-like wrapper for prometheus-net</Description>
    <PackageProjectUrl>https://github.com/Mikibot/Prometheus</PackageProjectUrl>
    <PackageLicenseUrl>https://github.com/Mikibot/Prometheus/blob/master/LICENSE</PackageLicenseUrl>
    <RepositoryUrl>https://github.com/Mikibot/Prometheus</RepositoryUrl>
    <RepositoryType>GIT</RepositoryType>
    <PackageTags>prometheus,prometheus-net,wrapper,</PackageTags>
    <PackageReleaseNotes>Initial release</PackageReleaseNotes>
    <Version>0.1.0</Version>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="prometheus-net" Version="2.1.3" />
  </ItemGroup>

</Project>
